  RowBox[{
   RowBox[{"FoldWhileList", "[", 
    RowBox[{
    "f_", ",", "test_", ",", "start_", ",", "secargs_List", ",", 
     "max_Integer"}], "]"}], ":=", 
   RowBox[{"FoldWhileList", "[", 
    RowBox[{"f", ",", 
     RowBox[{"dressInCtr", "[", 
      RowBox[{"test", ",", "max"}], "]"}], ",", "start", ",", "secargs"}], 
    "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"FoldWhileList", "[", 
