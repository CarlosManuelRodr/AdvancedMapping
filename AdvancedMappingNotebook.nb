    RowBox[{
     RowBox[{"result", " ", "=", " ", 
      RowBox[{
       RowBox[{"Reap", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Fold", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"test", "[", "##", "]"}], ",", "\[IndentingNewLine]", 
              RowBox[{"Sow", "[", 
               RowBox[{
                RowBox[{"f", "[", "##", "]"}], ",", "tag"}], "]"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{"Return", "[", 
               RowBox[{"Null", ",", "Fold"}], "]"}]}], "\[IndentingNewLine]", 
             "]"}], "&"}], ",", "\[IndentingNewLine]", "start", ",", 
           "\[IndentingNewLine]", "secargs"}], "\[IndentingNewLine]", "]"}], 
         ",", "\[IndentingNewLine]", "_", ",", "\[IndentingNewLine]", 
         RowBox[{"#2", "&"}]}], "\[IndentingNewLine]", "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
