 RowBox[{
  RowBox[{
   RowBox[{"dressInCtr", "[", 
    RowBox[{"test_", ",", "max_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ctr", "=", "0"}], "}"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"++", "ctr"}], "\[LessEqual]", "max"}], ")"}], "&&", 
       RowBox[{"test", "[", "##", "]"}]}], "&"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"FoldWhile", ",", "HoldFirst"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FoldWhile", "[", 
     RowBox[{
     "f_", ",", "test_", ",", "start_", ",", "secargs_List", ",", 
      "max_Integer"}], "]"}], ":=", 
    RowBox[{"FoldWhile", "[", 
     RowBox[{"f", ",", 
      RowBox[{"dressInCtr", "[", 
       RowBox[{"test", ",", "max"}], "]"}], ",", "start", ",", "secargs"}], 
     "]"}]}], ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FoldWhile", "[", 
     RowBox[{"f_", ",", "test_", ",", "start_", ",", "secargs_List"}], "]"}], 
    ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"last", "=", "start"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"Fold", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"test", "[", "##", "]"}], ",", 
           RowBox[{"last", "=", 
            RowBox[{"f", "[", "##", "]"}]}], ",", 
           RowBox[{"Return", "[", 
            RowBox[{"last", ",", "Fold"}], "]"}]}], "]"}], "&"}], ",", 
        "start", ",", "secargs"}], "]"}]}], "\[IndentingNewLine]", "]"}]}], 
   ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"FoldWhileList", ",", "HoldFirst"}], "]"}], ";"}], "\n", 
